{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "mi-sistema/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/db-viewer",
      "dest": "api/index.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/mi-sistema/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/mi-sistema/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/mi-sistema/manifest.json"
    },
    {
      "src": "/images/(.*)",
      "dest": "/mi-sistema/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/mi-sistema/$1",
      "continue": true
    }
  ]
}